ion in next, psncjog.options do if newOption.type == "\108\97\98\101\108" then n(newOption, psncjog) elseif newOption.type == "\116\111\103\103\108\101" then f_(newOption, psncjog) elseif newOption.type == "\98\117\116\116\111\110" then dk(newOption, psncjog) elseif newOption.type == "\108\105\115\116" then kq(newOption, psncjog, m) elseif newOption.type == "\98\111\120" then ha(newOption, psncjog) elseif newOption.type == "\98\105\110\100" then b(newOption, psncjog) elseif newOption.type == "\115\108\105\100\101\114" then _(newOption, psncjog) elseif newOption.type == "\99\111\108\111\114" then jo(newOption, psncjog, m) elseif newOption.type == "\102\111\108\100\101\114" then newOption:init() end end end local function i(ar) function ar:AddLabel(psncjog) psncjog = typeof(psncjog) == "\116\97\98\108\101" and psncjog or {} psncjog.text = tostring(psncjog.text) psncjog.type = "\108\97\98\101\108" psncjog.position = #self.options table.insert(self.options, psncjog) return psncjog end function ar:AddToggle(psncjog) psncjog = typeof(psncjog) == "\116\97\98\108\101" and psncjog or {} psncjog.text = tostring(psncjog.text) psncjog.state = typeof(psncjog.state) == "\98\111\111\108\101\97\110" and psncjog.state or false psncjog.callback = typeof(psncjog.callback) == "\102\117\110\99\116\105\111\110" and psncjog.callback or function() end psncjog.type = "\116\111\103\103\108\101" psncjog.position = #self.options psncjog.flag = psncjog.flag or psncjog.text cj.flags[psncjog.flag] = psncjog.state table.insert(self.options, psncjog) return psncjog end function ar:AddButton(psncjog) psncjog = typeof(psncjog) == "\116\97\98\108\101" and psncjog or {} psncjog.text = tostring(psncjog.text) psncjog.callback = typeof(psncjog.callback) == "\102\117\110\99\116\105\111\110" and psncjog.callback or function() end psncjog.type = "\98\117\116\116\111\110" psncjog.position = #self.options psncjog.flag = psncjog.flag or psncjog.text table.insert(self.options, psncjog) return psncjog end function ar:AddBind(psncjog) psncjog = typeof(psncjog) == "\116\97\98\108\101" and psncjog or {} psncjog.text = tostring(psncjog.text) psncjog.key = (psncjog.key and psncjog.key.Name) or psncjog.key or "\70" psncjog.hold = typeof(psncjog.hold) == "\98\111\111\108\101\97\110" and psncjog.hold or false psncjog.callback = typeof(psncjog.callback) == "\102\117\110\99\116\105\111\110" and psncjog.callback or function() end psncjog.type = "\98\105\110\100" psncjog.position = #self.options psncjog.flag = psncjog.flag or psncjog.text cj.flags[psncjog.flag] = psncjog.key table.insert(self.options, psncjog) return psncjog end function ar:AddSlider(psncjog) psncjog = typeof(psncjog) == "\116\97\98\108\101" and psncjog or {} psncjog.text = tostring(psncjog.text) psncjog.min = typeof(psncjog.min) == "\110\117\109\98\101\114" and psncjog.min or 0 psncjog.max = typeof(psncjog.max) == "\110\117\109\98\101\114" and psncjog.max or 0 psncjog.dual = typeof(psncjog.dual) == "\98\111\111\108\101\97\110" and psncjog.dual or false psncjog.value = math.clamp(typeof(psncjog.value) == "\110\117\109\98\101\114" and psncjog.value or psncjog.min, psncjog.min, psncjog.max) psncjog.value2 = typeof(psncjog.value2) == "\110\117\109\98\101\114" and psncjog.value2 or psncjog.max psncjog.callback = typeof(psncjog.callback) == "\102\117\110\99\116\105\111\110" and psncjog.callback or function() end psncjog.float = typeof(psncjog.value) == "\110\117\109\98\101\114" and psncjog.float or 1 psncjog.type = "\115\108\105\100\101\114" psncjog.position = #self.options psncjog.flag = psncjog.flag or psncjog.text cj.flags[psncjog.flag] = psncjog.value table.insert(self.options, psncjog) return psncjog end function ar:AddList(psncjog) psncjog = typeof(psncjog) == "\116\97\98\108\101" and psncjog or {} psncjog.text = tostring(psncjog.text) psncjog.values = typeof(psncjog.values) == "\116\97\98\108\101" and psncjog.values or {} psncjog.value = tostring(psncjog.value or psncjog.values[1] or "") psncjog.callback = typeof(psncjog.callback) == "\102\117\110\99\116\105\111\110" and psncjog.callback or function() end psncjog.open = false psncjog.type = "\108\105\115\116" psncjog.position = #self.options psncjog.flag = psncjog.flag or psncjog.text cj.flags[psncjog.flag] = psncjog.value table.insert(self.options, psncjog) return psncjog end function ar:AddBox(psncjog) psncjog = typeof(psncjog) == "\116\97\98\108\101" and psncjog or {} psncjog.text = tostring(psncjog.text) psncjog.value = tostring(psncjog.value or "") psncjog.callback = typeof(psncjog.callback) == "\102\117\110\99\116\105\111\110" and psncjog.callback or function() end psncjog.type = "\98\111\120" psncjog.position = #self.options psncjog.flag = psncjog.flag or psncjog.text cj.flags[psncjog.flag] = psncjog.value table.insert(self.options, psncjog) return psncjog end function ar:AddColor(psncjog) psncjog = typeof(psncjog) == "\116\97\98\108\101" and psncjog or {} psncjog.text = tostring(psncjog.text) psncjog.color = typeof(psncjog.color) == "\116\97\98\108\101" and Color3.new(tonumber(psncjog.color[1]), tonumber(psncjog.color[2]), tonumber(psncjog.color[3])) or psncjog.color or Color3.new(255, 255, 255) psncjog.callback = typeof(psncjog.callback) == "\102\117\110\99\116\105\111\110" and psncjog.callback or function() end psncjog.open = false psncjog.type = "\99\111\108\111\114" psncjog.position = #self.options psncjog.flag = psncjog.flag or psncjog.text cj.flags[psncjog.flag] = psncjog.color table.insert(self.options, psncjog) return psncjog end function ar:AddFolder(mbpsnc) local psncjog = {} psncjog.title = tostring(mbpsnc) psncjog.options = {} psncjog.open = false psncjog.type = "\102\111\108\100\101\114" psncjog.position = #self.options table.insert(self.options, psncjog) i(psncjog) function psncjog:init() e(self.title, ar.content, self, true) p(self, ar) end return psncjog end end function cj:CreateWindow(mbpsnc) local bpsncjo = {mbpsnc = tostring(mbpsnc), options = {}, open = true, canInit = true, init = false, sncjo = #self.windows} i(bpsncjo) table.insert(cj.windows, bpsncjo) return bpsncjo end local mbpsncj local imb function cj:Init() self.base = self.base or self:Create("\83\99\114\101\101\110\71\117\105") if syn and syn.protect_gui then syn.protect_gui(self.base) elseif get_hidden_gui then get_hidden_gui(self.base) end self.base.Parent = game:GetService"\67\111\114\101\71\117\105" self.cursor = self.cursor or self:Create("\70\114\97\109\101", { ZIndex = 100, AnchorPoint = Vector2.new(0, 0), Size = UDim2.new(0, 5, 0, 5), BackgroundColor3 = Color3.fromRGB(255, 255, 255), Parent = self.base }) for _, bpsncjo in next, self.windows do if bpsncjo.canInit and not bpsncjo.init then bpsncjo.init = true e(bpsncjo.title, self.base, bpsncjo) p(bpsncjo) end end end function cj:Close() self.open = not self.open self.cursor.Visible = self.open if self.activePopup then self.activePopup:Close() end for _, bpsncjo in next, self.windows do if bpsncjo.main then bpsncjo.main.Visible = self.open end end end bps.InputBegan:connect(function(jog) if jog.UserInputType == Enum.UserInputType.MouseButton1 then if cj.activePopup then if jog.Position.X < cj.activePopup.mainHolder.AbsolutePosition.X or jog.Position.Y < cj.activePopup.mainHolder.AbsolutePosition.Y then cj.activePopup:Close() end end if cj.activePopup then if jog.Position.X > cj.activePopup.mainHolder.AbsolutePosition.X + cj.activePopup.mainHolder.AbsoluteSize.X or jog.Position.Y > cj.activePopup.mainHolder.AbsolutePosition.Y + cj.activePopup.mainHolder.AbsoluteSize.Y then cj.activePopup:Close() end end end end) bps.InputChanged:connect(function(jog) if jog.UserInputType == Enum.UserInputType.MouseMovement and cj.cursor then local _imb = bps:GetMouseLocation() + Vector2.new(0, -36) cj.cursor.Position = UDim2.new(0, _imb.X - 2, 0, _imb.Y - 2) end if jog == dragInput and mbp then h(jog) end end) return library
